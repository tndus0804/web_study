package iotest;

import java.io.FileReader;

public class FileReaderTest1 {
	
	public static void main(String[] args) throws Exception {
		FileReader fr = new FileReader("test.txt");
		System.out.println(fr.read());
		System.out.println((char)fr.read());
		System.out.println((char)fr.read());
		System.out.println((char)fr.read());
		System.out.println((char)fr.read());
		System.out.println((char)fr.read());
		
		
	} // main
	
} // class
